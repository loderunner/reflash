{
  "name": "reflash",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/reflash.umd.cjs",
  "module": "./dist/reflash.js",
  "types": "./dist/index.d.ts",
